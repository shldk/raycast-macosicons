{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "macosicons",
  "title": "MacOSIcons.com",
  "description": "Raycast extension for browsing and applying icons from macOSicons.com",
  "icon": "main-icon.png",
  "type": "module",
  "author": "shldk",
  "categories": [
    "System",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "explore-icons",
      "title": "Explore Icons",
      "description": "Explore icons on macOSIcons.com",
      "mode": "view"
    },
    {
      "name": "icon-history",
      "title": "Icon History",
      "description": "Show icon history",
      "mode": "view",
      "icon": "history-icon.png"
    },
    {
      "name": "restore-default-icon",
      "title": "Restore Default Icon",
      "description": "Restore application's default ccon",
      "mode": "view"
    },
    {
      "name": "fix-outdated-icons",
      "title": "Fix Outdated Icons",
      "description": "Fix outdated icons after updates",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.89.1",
    "@raycast/utils": "^1.18.1",
    "fuzzysort": "^3.1.0",
    "got": "^14.4.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.18",
    "eslint": "^8.57.1",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "start": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
