{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "macosicons",
  "title": "MacOSIcons.com",
  "description": "Raycast extension for browsing and applying icons from macOSicons.com",
  "icon": "main-icon.png",
  "author": "shldk",
  "categories": [
    "System",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "browse",
      "title": "Browse Custom Icons",
      "description": "Change icon for an application",
      "mode": "view"
    },
    {
      "name": "history",
      "title": "Show Icon History",
      "description": "Show icon change history",
      "mode": "view",
      "icon": "history-icon.png"
    },
    {
      "name": "restore",
      "title": "Restore Custom Icons",
      "description": "Restore custom icons after application updates",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "@raycast/utils": "^1.10.1",
    "algoliasearch": "^4.20.0",
    "fuzzysort": "^3.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.9.0",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "react-devtools": "^4.28.4",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "start": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
