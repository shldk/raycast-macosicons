{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "macosicons",
  "title": "macOSicons",
  "description": "Raycast extension for browsing and applying icons from macOSicons.com",
  "icon": "command-icon.png",
  "author": "Skate",
  "categories": [
    "System",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "browse",
      "title": "Browse Icons",
      "description": "Show icons from macOSicons.com",
      "mode": "view"
    },
    {
      "name": "history",
      "title": "Show Icon History",
      "description": "Show history of all icon changes performed by extension",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.2",
    "@raycast/utils": "^1.4.8",
    "algoliasearch": "^4.20.0",
    "@react-buddy/ide-toolbox": "^2.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^20.9.0",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "react-devtools": "^4.28.4",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
